		<url-pattern>/facultySave</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>dormitorySaveServlet</servlet-name>
		<url-pattern>/dormitorySave</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>gradeSaveServlet</servlet-name>
		<url-pattern>/gradeSave</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>studentSaveServlet</servlet-name>
		<url-pattern>/studentSave</url-pattern>
	</servlet-mapping>
